server:
  port: 8087

spring:
  mail:
    host: smtp.gmail.com
    port: 587
    username: <username>
    password: <password>
    properties:
      mail:
        smtp:
          auth: true
          starttls:
            enable: true
  rabbitmq:
    host: localhost
    port: 5672
    username: <username>
    password: <password>
    queues:
      registration-mail: registration_mail

service:
  activation:
    uri: http://127.0.0.1:8086/users/activation?id={id}